{
  "name": "My Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  
"background": {
    "run_at": "document_start",
    "scripts": [
        "bookmarks.js"
    ],
    "persistent":false
},
"browser_action": {
  "default_icon": {
    "19": "icon128.png"
  },
  "default_title": "Bookmark Reader",
  "default_popup": "/popup/popup.html"
},
  "permissions": [
    "bookmarks",
    "tabs",
    "background",
    "storage",
    "http://*/*",
    "https://*/*"
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}